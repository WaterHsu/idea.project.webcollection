<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  
  
   <servlet>
  	<servlet-name>Checklogin</servlet-name>
  	<servlet-class>com.database.xpf.first.manage.login.Checklogin</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>Addkind</servlet-name>
  	<servlet-class>com.database.xpf.first.manage.collection.Addkind</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>Addweb</servlet-name>
  	<servlet-class>com.database.xpf.first.manage.web.Addweb</servlet-class>
  </servlet>
  
  
  
<servlet-mapping>
	<servlet-name>Checklogin</servlet-name>
	<url-pattern>/Checklogin</url-pattern>
</servlet-mapping>

<servlet-mapping>
	<servlet-name>Addkind</servlet-name>
	<url-pattern>/Addkind</url-pattern>
</servlet-mapping>

<servlet-mapping>
	<servlet-name>Addweb</servlet-name>
	<url-pattern>/Addweb</url-pattern>
</servlet-mapping>
  
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
